{
    "name": "@1inch/mooniswap",
    "version": "0.0.1",
    "description": "",
    "repository": {
        "type": "git",
        "url": "git@github.com:CryptoManiacsZone/mooniswap.git"
    },
    "license": "MIT",
    "dependencies": {
        "bignumber.js": "^9.0.0",
        "dotenv": "^8.2.0",
        "web3": "^1.3.0"
    },
    "devDependencies": {
        "@galtproject/eslint-config-galt": "github:galtproject/eslint-config-galt#1e45e18b5e6849f6a88036a151f62d60889e9396",
        "@galtproject/solidity-test-chest": "https://github.com/galtproject/solidity-test-chest#f93100a",
        "@codechecks/client": "^0.1.10",
        "@openzeppelin/contract-loader": "^0.6.1",
        "@openzeppelin/contracts": "^3.0.2",
        "@openzeppelin/test-environment": "^0.1.5",
        "@openzeppelin/test-helpers": "^0.5.6",
        "@truffle/debug-utils": "^4.2.8",
        "@truffle/hdwallet-provider": "^1.1.0",
        "chai": "^4.2.0",
        "cli-table": "^0.3.1",
        "coveralls": "^3.1.0",
        "eslint": "^7.5.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.0.1",
        "eslint-plugin-standard": "^4.0.0",
        "eth-gas-reporter": "^0.2.17",
        "ganache-cli": "^6.9.1",
        "mocha": "^8.1.3",
        "solc": "^0.6.12",
        "solhint": "^3.0.0",
        "solidity-coverage": "^0.7.9",
        "truffle": "^5.1.35",
        "truffle-flattener": "^1.4.4"
    },
    "engines": {
        "node": "^12.18.0"
    },
    "keywords": [
        "solidity",
        "ethereum",
        "smart",
        "contracts"
    ],
    "scripts": {
        "test": "mocha --exit --recursive test --timeout 30000",
        "ttest": "truffle test --network test",
        "compile": "truffle compile",
        "deploy": "truffle migrate --network local",
        "deploy_crowdsale_local": "truffle migrate --network local --f 1 --to 2",
        "deploy_crowdsale_ropsten": "truffle migrate --network ropsten --f 1 --to 2",
        "deploy_crowdsale_kovan": "truffle migrate --network kovan --f 1 --to 2",
        "test2": "truffle test --stacktrace",
        "coverage": "truffle run coverage",
        "coveralls": "truffle run coverage && cat coverage/lcov.info | coveralls",
        "lint:js": "eslint .",
        "lint:js:fix": "eslint . --fix",
        "lint:sol": "solhint --max-warnings 0 \"contracts/**/*.sol\"",
        "lint:sol:fix": "solhint --max-warnings 0 \"contracts/**/*.sol\" --fix",
        "lint": "yarn run lint:js && yarn run lint:sol",
        "lint:fix": "yarn run lint:js:fix && yarn run lint:sol:fix",
        "dist": "truffle-flattener ./contracts/Mooniswap.sol |  awk '/SPDX-License-Identifier/&&c++>0 {next} 1' > ./Mooniswap.full.sol && solcjs --bin --abi --optimize ./Mooniswap.full.sol && mv ./__Mooniswap_full_sol_Mooniswap.bin ./Mooniswap.full.bin && mv ./__Mooniswap_full_sol_Mooniswap.abi ./Mooniswap.full.abi && rm ./*_sol_*",
        "dist:factory": "truffle-flattener ./contracts/MooniFactory.sol |  awk '/SPDX-License-Identifier/&&c++>0 {next} 1' > ./MooniFactory.full.sol && solcjs --bin --abi --optimize ./MooniFactory.full.sol && mv ./__MooniFactory_full_sol_MooniFactory.bin ./MooniFactory.full.bin && mv ./__MooniFactory_full_sol_MooniFactory.abi ./MooniFactory.full.abi && rm ./*_sol_*"
    }
}
